{
  "name": "Project Folder",
  "id": "project_folder",
  "description": "A custom feature to create a mounted test folder.",
  "version": "0.0.1",
  "mounts": [
    {
      "source": "../.vscode",
      "target": "/home/vscode/.vscode",
      "type": "bind"
    },
    {
      "source": "../llm_examples",
      "target": "/home/vscode/development/llm_examples",
      "type": "bind"
    },
    {
      "source": "../src",
      "target": "/home/vscode/development/src",
      "type": "bind"
    },
    {
      "source": "../tests",
      "target": "/home/vscode/development/tests",
      "type": "bind"
    },
    {
      "source": "../__main__.py",
      "target": "/home/vscode/development/__main__.py",
      "type": "bind"
    },
    {
      "source": "../.env.example",
      "target": "/home/vscode/development/.env.example",
      "type": "bind"
    },
    {
      "source": "../.gitignore",
      "target": "/home/vscode/development/.gitignore",
      "type": "bind"
    },
    {
      "source": "../config.py",
      "target": "/home/vscode/development/config.py",
      "type": "bind"
    },
    {
      "source": "../config.yaml",
      "target": "/home/vscode/development/config.yaml",
      "type": "bind"
    },
    {
      "source": "../Makefile",
      "target": "/home/vscode/development/Makefile",
      "type": "bind"
    },
    {
      "source": "../poetry.toml",
      "target": "/home/vscode/development/poetry.toml",
      "type": "bind"
    },
    {
      "source": "../pyproject.toml",
      "target": "/home/vscode/development/pyproject.toml",
      "type": "bind"
    },
    {
      "source": "../README.md",
      "target": "/home/vscode/development/README.md",
      "type": "bind"
    }
  ]
}