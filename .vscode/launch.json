{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "NASA CMR AI Agent - __main__.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/__main__.py",
      "console": "integratedTerminal"
    },
    {
      "name": "NASA CMR AI Agent - Module with uvicorn",
